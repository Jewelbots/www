				(function(d){
					var code_video_iframe = d.getElementById('code_video_iframe');
					code_video_iframe.src ='https://player.vimeo.com/video/172983545?autoplay=1&loop=1&title=0&byline=0&portrait=0';
				})(document);

			}
			if(state=='size'){
				//-- 2: size video
				if($(window).width() > 1024){
					$('.index .section_code').css('height', $('.index .section_code').width() /1.78 );
					$('.index .section_code #code_video_iframe').css('height', $('.index .section_code').width() /1.78 );
				} else {
					$('.index .section_code').css('height', 'auto' );
					$('.index .section_code #code_video_iframe').css('height', $('.index .section_code').width() /1.78 );
				}
			}
		}
